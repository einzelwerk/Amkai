section.section.feedback-section.container
    .feedback__heading
        h1.feedback__title am kai Impressionen
    .feedback__content-first
        p.feedback__first-text “Die Gerichte sind kreativ, von höchster Qualität und einfach unheimlich lecker.”
        .feedback__first-one
            a.feedback__link(href=require('@/images/feedback/feedback-1.png') data-fancybox='gallery') 
                picture 
                    source(media="(min-width: 1026px)" srcset=require('@/images/feedback/feedback-1.png'))
                    source(media="(min-width: 576px) and (max-width: 1025px)" srcset=require('@/images/feedback/feedback-1-md.jpg'))
                    source(media="(max-width: 575px)" srcset=require('@/images/feedback/feedback-1-sm.jpg'))
                    img(src=require('@/images/feedback/feedback-1.png') alt='Picture 1')
